{
  "name": "rdap",
  "version": "0.0.14",
  "description": "Modern RDAP client and server with RFC compliance, bootstrap service, and h3 integration",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepack": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funish/axis.git"
  },
  "keywords": [
    "rdap",
    "whois",
    "domain",
    "ip",
    "asn",
    "registry",
    "icann",
    "rfc",
    "bootstrap",
    "h3",
    "server",
    "client"
  ],
  "author": {
    "name": "Funish",
    "email": "contact@funish.net",
    "url": "http://www.funish.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funish/axis/issues"
  },
  "homepage": "https://github.com/funish/axis#readme",
  "dependencies": {
    "h3": "2.0.1-rc.5",
    "ipdo": "workspace:*",
    "ofetch": "1.4.1"
  }
}
