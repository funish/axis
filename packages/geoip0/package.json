{
  "name": "geoip0",
  "version": "0.0.7",
  "description": "Unified geolocation information query library with multi-provider support",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./drivers/*": {
      "import": "./dist/drivers/*.mjs",
      "require": "./dist/drivers/*.cjs",
      "types": "./dist/drivers/*.d.ts"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    },
    "./mmdb": {
      "import": "./dist/mmdb.mjs",
      "require": "./dist/mmdb.cjs",
      "types": "./dist/mmdb.d.ts"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepack": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funish/axis.git"
  },
  "keywords": [
    "geolocation",
    "ip",
    "geoip",
    "location",
    "ip-address",
    "geolocation-api",
    "ipv4",
    "ipv6",
    "ip-location",
    "asn",
    "isp",
    "typescript"
  ],
  "author": {
    "name": "Funish",
    "email": "contact@funish.net",
    "url": "http://www.funish.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funish/axis/issues"
  },
  "homepage": "https://github.com/funish/axis#readme",
  "dependencies": {
    "h3": "2.0.1-rc.5",
    "ipdo": "workspace:*",
    "ofetch": "1.4.1",
    "undio": "0.2.0"
  }
}
