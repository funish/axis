{
  "name": "undns",
  "version": "0.0.11",
  "description": "Unified DNS library with multi-driver support, DoH server, and comprehensive record management",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./drivers/*": {
      "import": "./dist/drivers/*.mjs",
      "require": "./dist/drivers/*.cjs",
      "types": "./dist/drivers/*.d.ts"
    },
    "./servers/*": {
      "import": "./dist/servers/*.mjs",
      "require": "./dist/servers/*.cjs",
      "types": "./dist/servers/*.d.ts"
    }
  },
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepack": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funish/axis.git"
  },
  "keywords": [
    "dns",
    "dns-management",
    "doh",
    "rfc8484",
    "cloudflare",
    "vercel",
    "dnspod",
    "domain",
    "records",
    "typescript",
    "nodejs",
    "h3",
    "tlsa",
    "naptr"
  ],
  "author": {
    "name": "Funish",
    "email": "contact@funish.net",
    "url": "http://www.funish.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funish/axis/issues"
  },
  "homepage": "https://github.com/funish/axis#readme",
  "peerDependencies": {
    "cloudflare": "^5.2.0"
  },
  "peerDependenciesMeta": {
    "cloudflare": {
      "optional": true
    }
  },
  "dependencies": {
    "h3": "2.0.1-rc.5",
    "ipdo": "workspace:*",
    "ofetch": "1.4.1"
  }
}
